    <script type="text/javascript">


        var agent = navigator.userAgent.toLowerCase();
        var nazwa = "nieznany";
        if(agent.indexOf('windows')   
        {
            nazwa = "Windows";
        }
        if(agent.indexOf('linux')        
        {
            nazwa = "Linux";
        }
        else if(agent.indexOf('mac') 
        {
            nazwa = "Mac";
        }                
        document.write("tw√≥j system to: " + nazwa);
    </script>
  </body>
